FoamFile
{
    version     2.0;
    format      ascii;
    class       volVectorField;
    location    "0.0012";
    object      Vorticity;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 0 -1 0 0 0 0];

internalField   uniform (0 0 0);

boundaryField
{
    wall
    {
        type            zeroGradient;
    }
    walls
    {
        type            zeroGradient;
    }
    outlet
    {
        type            inletOutlet;
        inletValue      uniform (0 0 0);
        value           uniform (0 0 0);
    }
    inletWall
    {
        type            zeroGradient;
    }
    inlet
    {
        type            zeroGradient;
    }
}


